{
  "apiVersion": "v1",
  "spec": {
    "kind": "Workflow",
    "spec": {
      "type": "sequential",
      "steps": [
        {
          "kind": "Addon",
          "spec": {
            "name": "incandescent-reading",
            "description": "",
            "type": "postgres",
            "version": "14.5.0",
            "billing": {
              "deploymentPlan": "nf-compute-50",
              "storageClass": "ssd",
              "storage": 4096,
              "replicas": 1
            },
            "tlsEnabled": true,
            "externalAccessEnabled": false,
            "ipPolicies": [],
            "pitrEnabled": false
          }
        },
        {
          "kind": "SecretGroup",
          "spec": {
            "name": "Example Secret",
            "secretType": "environment",
            "priority": 10,
            "secrets": {
              "variables": {
                "HA_ACTIVE": "true",
                "DB_TYPE": "postgres"
              }
            },
            "addonDependencies": [
              {
                "addonId": "incandescent-reading",
                "keys": [
                  {
                    "keyName": "USERNAME",
                    "aliases": [
                      "DB_USER"
                    ]
                  },
                  {
                    "keyName": "PASSWORD",
                    "aliases": [
                      "DB_PASS"
                    ]
                  },
                  {
                    "keyName": "DATABASE",
                    "aliases": [
                      "DB_NAME"
                    ]
                  },
                  {
                    "keyName": "HOST",
                    "aliases": [
                      "DB_HOST"
                    ]
                  },
                  {
                    "keyName": "PORT",
                    "aliases": [
                      "DB_PORT"
                    ]
                  },
                  {
                    "keyName": "TLS_ENABLED",
                    "aliases": [
                      "DB_SSL"
                    ]
                  }
                ]
              }
            ],
            "description": "",
            "restrictions": {
              "restricted": false,
              "nfObjects": []
            }
          }
        },
        {
          "kind": "DeploymentService",
          "spec": {
            "name": "cuddly-bucket",
            "billing": {
              "deploymentPlan": "nf-compute-50"
            },
            "deployment": {
              "instances": 1,
              "storage": {
                "ephemeralStorage": {
                  "storageSize": 1024
                }
              },
              "external": {
                "imagePath": "requarks/wiki:latest"
              }
            },
            "ports": [
              {
                "name": "p01",
                "internalPort": 3000,
                "public": true,
                "protocol": "HTTP",
                "security": {
                  "credentials": [],
                  "policies": []
                },
                "domains": [],
                "disableNfDomain": false
              },
              {
                "name": "p02",
                "internalPort": 3443,
                "protocol": "TCP",
                "public": false,
                "security": {
                  "credentials": [],
                  "policies": []
                },
                "domains": [],
                "disableNfDomain": false
              }
            ],
            "healthChecks": [],
            "description": "",
            "runtimeEnvironment": {}
          }
        }
      ]
    }
  },
  "description": "",
  "name": "fond-bell",
  "project": {
    "id": "wikijs-template"
  }
}